{
    "compilerOptions": {
        "incremental": true,
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "outDir": "./lib",
        "baseUrl": ".",
        "target": "ES2019",
    },
    "paths": {
      "meta": ["meta/index.js"],
      "meta/*": ["meta/*"]
    },
    "exclude": ["node_modules"],
    "include": ["aspects/**/*.ts", "meta/**/*.ts"]
}
