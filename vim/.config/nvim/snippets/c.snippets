# Print to standard out, with newline
snippet coutn
	printf("$0\n");

# Print to standard out, without newline
snippet cout
	printf("$0");

# Print to standard error
snippet cerr
	fprintf(stderr, "%0\n");

# <> include
snippet inc
	#include <$0>

# "" include
snippet qinc
	#include "$0"

# include guard
snippet guard
	#ifndef ${1:guard}
	#define $1

	$0

	#endif
